{
  "name": "www-covidcast-classic",
  "version": "2.6.7",
  "private": true,
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/cmu-delphi/www-covidcast-classic",
  "bugs": {
    "url": "https://github.com/cmu-delphi/www-covidcast-classic/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmu-delphi/www-covidcast-classic.git"
  },
  "main": "public/bundle.js",
  "files": [
    "public/**.js",
    "public/**.map",
    "public/**.txt",
    "public/**.css",
    "!public/wrapper.*"
  ],
  "scripts": {
    "clean": "rimraf \"*.zip\" \"*.tgz\" public",
    "compile": "tsc -p tsconfig.json --noEmit",
    "gen": "node ./scripts/generateDescriptions.js",
    "gen:check": "npm run gen -- --check",
    "gen:local": "npm run gen -- --localOnly",
    "gen:fallback": "npm run gen -- --fallback",
    "start": "npm run gen:fallback && node --max-old-space-size=6192 node_modules/webpack-cli/bin/cli.js serve --mode development --hot --devtool eval-cheap-module-source-map",
    "prebuild": "npm run clean",
    "build": "npm run gen:fallback && cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development -w",
    "prettier": "prettier --write \"**/*\"",
    "lint": "npm run gen:check && eslint src --ext .js,.svelte && prettier --check \"**/*\"",
    "fix": "eslint src --ext .js,.svelte --fix && prettier --write \"**/*\"",
    "test": "jest --passWithNoTests",
    "prepare": "husky install",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@popperjs/core": "^2.9.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@turf/bbox": "^6.4.0",
    "@types/d3-color": "^3.1.0",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-format": "^3.0.1",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-time": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/jest": "^27.0.0",
    "@types/lodash-es": "^4.17.4",
    "@types/uikit": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compute-logspace": "^1.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "d3-color": "^3.1.0",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-time": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "eslint": "^7.29.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-svelte3": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "js-cookie": "^2.2.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "lodash-es": "^4.17.21",
    "mapbox-gl": "^1.13.0",
    "marked": "^4.0.10",
    "mini-css-extract-plugin": "^2.0.0",
    "node-fetch": "^2.6.7",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.13.7",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.2.0",
    "svelte-icons": "^2.1.0",
    "svelte-jester": "^2.3.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.3",
    "topojson-client": "^3.1.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "vega": "^5.23.0",
    "vega-embed": "^6.18.2",
    "vega-lite": "^5.1.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "uikit": "^3.7.0"
  },
  "lint-staged": {
    "*.{ts,js,svelte}": "eslint --cache --fix",
    "*.{ts,js,css,md,svelte}": "prettier --write"
  }
}
